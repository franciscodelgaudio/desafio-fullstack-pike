{
  "compilerOptions": {
    "target": "ES2017",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./src/*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts", "src/app/dashboard/server/actions.js", "src/app/dashboard/client/LerTarefa.jsx", "src/app/dashboard/client/ExcluirTarefa.jsx", "src/app/dashboard/client/AtualizarTarefa.jsx", "src/app/dashboard/client/dashboard.jsx", "src/app/register/client/register.jsx", "src/app/dashboard/client/CriarTarefa.jsx", "src/lib/connectiondb.js", "src/app/register/page.jsx", "src/app/login/client/login.jsx", "src/app/register/server/actions.js", "src/app/login/server/actions.js"],
  "exclude": ["node_modules"]
}
